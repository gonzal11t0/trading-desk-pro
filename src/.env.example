# .env.example
# =============== API KEYS ===============
# Consigue tus keys gratuitas aquí:

# Alpha Vantage (Stocks/Forex): https://www.alphavantage.co/support/#api-key
VITE_ALPHA_VANTAGE_KEY=tu_key_aqui

# ScraperAPI (para scraping financiero): https://www.scraperapi.com/pricing
VITE_SCRAPERAPI_KEY=tu_key_aqui

# NewsAPI (Noticias): https://newsapi.org/register
VITE_NEWSAPI_KEY=tu_key_aqui

# =============== CONFIGURACIÓN ===============
VITE_APP_ENV=development
VITE_API_TIMEOUT=10000
VITE_BCRA_ENABLED=true
VITE_BCRA_TIMEOUT=15000

# ============================================
# EJEMPLO - NO USAR EN PRODUCCIÓN
