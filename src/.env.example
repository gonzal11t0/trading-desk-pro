# Copiar este archivo a .env y completar

# ADMINISTRADOR
VITE_ADMIN_USER=admin@ejemplo.com
VITE_ADMIN_PASS=Admin123!@#

# CLIENTES (formato: email:contraseña)
VITE_USER_1=cliente1@ejemplo.com:ClaveSegura123!
VITE_USER_2=cliente2@ejemplo.com:OtraClave#456
VITE_USER_3=
# ... hasta VITE_USER_10

# CONFIGURACIÓN
VITE_SESSION_TIMEOUT=60
VITE_REMEMBER_DAYS=30

